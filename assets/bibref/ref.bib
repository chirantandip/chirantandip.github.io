issn = {10445803},
journal = {Materials Characterization},
keywords = {Aging,High strength steel,Planar glide,Precipitation,Tensile properties},
mendeley-groups = {Steel B2 5%Ni},
number = {December 2020},
pages = {111291},
publisher = {Elsevier Inc.},
title = {{Precipitation behavior of B2 and $\kappa$-carbide during aging and its effect on mechanical properties in Al-containing high strength steel}},
