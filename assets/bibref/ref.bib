keywords = {Lagrange formalism,Multiphase,Phase-field},
mendeley-groups = {ICME/Stienbach},
number = {4},
pages = {385--393},
title = {{A generalized field method for multiphase transformations using interface fields}},
volume = {134},
year = {1999}
}
