issn = {13596454},
journal = {Acta Materialia},
keywords = {Cobalt-based superalloys,Morphology evolution,Phase field model},
mendeley-groups = {ICME/Voorhees},
pages = {117978},
publisher = {Elsevier Ltd},
title = {{Co-Based superalloy morphology evolution: A phase field study based on experimental thermodynamic and kinetic data}},
