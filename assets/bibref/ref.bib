journal = {Materials Science and Engineering A},
keywords = {B2,Light-weight,Low-density steel,Ni3Al,Ordering},
mendeley-groups = {Steel B2 5%Ni},
number = {April},
pages = {143577},
publisher = {Elsevier B.V.},
title = {{Enhancement of strength and ductility of Fe–13Al–16Mn–5Ni −0.8C steel by annealing at high temperature}},
